<tr>
  <td>
    <%= mode.id %>
  </td>
  <td>
    <%= mode.difficulty %>
  </td>
  <td>
    <%= mode.description %>
  </td>
  <td>
    <%= link_to t('defaults.show'), admins_mode_path(mode), class: 'btn btn-info' %>
    <%= link_to t('defaults.edit'), edit_admins_mode_path(mode), class: 'btn btn-success' %>
    <%= link_to t('defaults.delete'), admins_mode_path(mode), method: :delete, data: { confirm: t('defaults.message.delete_confirm') }, class: 'btn btn-danger' %>
  </td>
</tr>