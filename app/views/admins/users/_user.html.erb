<tr>
  <td>
    <%= user.id %>
  </td>
  <td>
    <%= user.name %>
  </td>
  <td>
    <%= user.email %>
  <td>
    <%= link_to t('defaults.show'), admins_user_path(user), class: 'btn btn-info' %>
    <%= link_to t('defaults.edit'), edit_admins_user_path(user), class: 'btn btn-success' %>
    <%= link_to t('defaults.delete'), admins_user_path(user), method: :delete, data: { confirm: t('defaults.message.delete_confirm') }, class: 'btn btn-danger' %>
  </td>
</tr>